<config>
    <global>
        <modules>
            <Virtua_MailModule>
                <version>0.1.0</version>
            </Virtua_MailModule>
        </modules>
        <blocks>
            <adminhtml>
                <rewrite>
                    <sales_order_grid>Virtua_MailModule_Test_Block_Adminhtml_Order_Grid</sales_order_grid>
                </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <virtua_mailmodule>
                <class>Virtua_MailModule_Helper</class>
            </virtua_mailmodule>
        </helpers>
    </global>
</config>